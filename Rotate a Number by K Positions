#include <stdio.h>

int main() {
    int n, k;
    printf("Enter number: ");
    scanf("%d", &n);
    printf("Enter k: ");
    scanf("%d", &k);

    int temp = n, count = 0;
    while (temp > 0) {
        count++;
        temp /= 10;
    }

    k = k % count;
    if(k<0) k=count+k; 	
    int x = 1, y = 1;
    for (int i = 0; i < k; i++) x *= 10;
    for (int i = 0; i < count - k; i++) y *= 10;

    int rem = n % y;
    int q = n / y;
    int rot = rem * x + q;

    printf("%d", rot);
    return 0;
}
